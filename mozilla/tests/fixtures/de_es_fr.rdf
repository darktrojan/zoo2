<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
	<Description about="urn:mozilla:install-manifest">
		<em:name>Name in English</em:name>
		<em:description>Description in English</em:description>

		<em:localized>
			<Description>
				<em:locale>de</em:locale>
				<em:name>Name in German</em:name>
				<em:description>Description in German</em:description>
			</Description>
		</em:localized>

		<em:localized>
			<Description>
				<em:locale>es</em:locale>
				<em:name>Name in Spanish</em:name>
				<em:description>Description in Spanish</em:description>
			</Description>
		</em:localized>

		<em:localized>
			<Description>
				<em:locale>fr</em:locale>
				<em:name>Name in French</em:name>
				<em:description>Description in French</em:description>
			</Description>
		</em:localized>
	</Description>
</RDF>
